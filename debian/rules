#!/usr/bin/make -f

DEB_DIVERT_EXTENSION = .sr

include /usr/share/cdbs/1/rules/debhelper.mk
include /usr/share/cdbs/1/rules/config-package.mk
include /usr/share/cdbs/1/rules/divert.mk

DOCS_sr-router-captive-portal = docs/sr_portal_status.1.gz docs/sr_portal_grant.1.gz docs/sr_portal_revoke.1.gz

%.gz: %.man
	gzip -c $< > $@

build/sr-router-captive-portal:: $(DOCS_sr-router-captive-portal)

override_dh_clean:
	dh_clean
	rm $(DOCS_sr-router-captive-portal)
